
.ORG 0x0000
LDI.W SP, 0x6000

; Setup Source at 8208 with 12
LDI.W R1, 12
LDI.W R2, 8208
ST.W R1, [R2, 0]

; Copy Logic (Generated Style)
; R1 = Src = 8208
LDI.W R1, 8208
; R2 = Dest = 8200
LDI.W R2, 8200
; R3 = Index = 0
LDI.W R3, 0

loop:
; R4 = Size = 1
LDI.W R4, 1
CMP.W R3, R4
BEQ end

; Cpy
LD.W R4, [R1, 0]
ST.W R4, [R2, 0]

; Inc
LDI.W R4, 1
ADD.W R1, R1, R4
ADD.W R2, R2, R4
ADD.W R3, R3, R4
JMP loop

end:
HLT
